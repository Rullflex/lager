extends modules/layout/_layout

prepend head
	title ДЕТСКИЙ ВОЕННО-СПОРТИВНЫЙ ЛАГЕРЬ “ФОРПОСТ” В ПОДМОСКОВЬЕ
	meta(name="description", content="ДЕТСКИЙ ВОЕННО-СПОРТИВНЫЙ ЛАГЕРЬ “ФОРПОСТ” В ПОДМОСКОВЬЕ")

block header
	header.header(data-uk-sticky="top: 100vh; animation: uk-animation-slide-top;")
		.container 
			.header__row 
				a.header__logo(href="/")
					p.header__logo-text На Главную
					picture 
						source(media=`(min-width: ${lg}px)` srcset=ipl)
						source(media=`(min-width: 0px)` srcset!=`${assets}logo-sm.webp`  type=`image/webp`)
						source(media=`(min-width: 0px)` srcset!=`${assets}logo-sm.png`  type=`image/png`)
						img(src=ipl alt='').header__logo-img
				ul.header__menu
					li.header__menu-item
						a.header__menu-link(href="#about" data-uk-scroll) О нас
					li.header__menu-item
						a.header__menu-link(href="#tour" data-uk-scroll) Смены
					li.header__menu-item
						a.header__menu-link(href="#accommodation" data-uk-scroll="offset: 30") Проживание
					li.header__menu-item
						a.header__menu-link(href="#news" data-uk-scroll="offset: 30") Новости
					li.header__menu-item
						a.header__menu-link(href="#contacts" data-uk-scroll) Контакты
				.header__phones 
					a.addit(href=`tel:+79661314129`) +7 (966) 131-41-29
					a(href=`tel:+74957659016`) +7 (495) 765-90-16
				a.header__burger.uk-icon(href="#")
					- for (var x = 0; x < 4; x++) 
						i
				.header__drop(data-uk-dropdown="mode: click; pos: bottom-right; boundary: .header; boundary-align: true; animation: uk-animation-slide-right-small; duration: 300")
					.header__drop-main
						ul.header__drop-menu
							li.header__drop-item
								a.header__drop-link(href="#" data-uk-scroll) Программа
							li.header__drop-item
								a.header__drop-link(href="#" data-uk-scroll) Стоимость
							li.header__drop-item
								a.header__drop-link(href="#news" data-uk-scroll="offset: 30") Новости
							li.header__drop-item
								a.header__drop-link(href="#accommodation" data-uk-scroll="offset: 30") Проживание
						a.header__drop-tel(href=`tel:+74957659016`) +7 (495) 765-90-16

